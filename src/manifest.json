{
  "name": "beslist.nl Team Website Tools",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "beslist48.png"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "<all_urls>"
  ],
  "icons": {
    "16": "beslist16.png",
    "48": "beslist48.png",
    "128": "beslist128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
